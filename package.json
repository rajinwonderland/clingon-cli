{
  "name": "clingon-cli",
  "description": "CLIngON allows you to scaffold frontmatter and/or other metadata into your mdx or markdown files.",
  "version": "0.0.7",
  "author": "rajinwonderland @rajinwonderland",
  "bin": {
    "clingon": "./bin/run"
  },
  "bugs": "https://github.com/rajinwonderland/clingon/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "clipboardy": "^2.0.0",
    "corejs": "^1.0.0",
    "cuid": "^2.1.6",
    "gradient-string": "^1.2.0",
    "inquirer": "^6.3.1",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.5.0",
    "ora": "^3.4.0",
    "slugify": "^1.3.4",
    "tslib": "^1",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^6.0.2",
    "@types/node": "^10",
    "@types/node-fetch": "^2.3.3",
    "@types/ora": "^3.2.0",
    "@types/validator": "^10.11.0",
    "globby": "^8",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/rajinwonderland/clingon-cli",
  "keywords": [
    "oclif",
    "frontatter",
    "mdx",
    "markdown",
    "cli generator",
    "frontmatter generator"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "clingon",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/rajinwonderland/clingon-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b -f && oclif-dev manifest && yarn jsonfiles",
    "readme": "oclif-dev readme",
    "jsonfiles": "cp ./src/utils/authors.json ./lib/utils",
    "prerelease": "oclif-dev pac && oclif-dev publish",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
